    <script type="text/javascript">
        function onbuttonclick(partner_Element,product_name,producttype)
        {
         /*   alert(producttype);*/
            var order_partner_name = $("#" + partner_Element + "").val();
            /*alert(order_partner_name);*/

            $.getJSON('@Url.Content("~/")' + "Dwq/GetInCompleteOrderId?productType=" + producttype+"&reasoncodeName=" + product_name + "&partnerName=" + order_partner_name, function (OrderId_Data)
            {

                debugger;
                window.location.href = "/orderprocess/Index/?orderId=" + OrderId_Data+"&returnurl=incomdwq";

            });


        }
    </script>

