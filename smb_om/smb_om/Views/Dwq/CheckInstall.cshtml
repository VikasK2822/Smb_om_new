    <script type="text/javascript">
        function onbuttonclick(partner_Element,product_name,product_Queue_type)
        {

            var order_partner_name = $("#" + partner_Element + "").val();
            alert(order_partner_name);
            alert(product_name);
            alert(product_Queue_type);

            $.getJSON('@Url.Content("~/")' + "Dwq/GetCheckInstallOrderId?productType=" + product_name + "&queueName=" + product_Queue_type + "&partnerName=" + order_partner_name, function (OrderId_Data)
            {

                if (OrderId_Data != '') {
                    window.location.href = "/orderprocess/Index/?orderId=" + OrderId_Data + "&returnurl=checkInstalldwq";
                }
                else
                {
                    alert('No Order Id Found');
                }

